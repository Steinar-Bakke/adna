        AdapterRemoved = 3;}
    else if ((AdapterFound == true) && (AdapterFound2 == false)){
        
            PrintLongestArray.push_back(LongestAdapter);
            PrintLongestArrayLength.push_back(LongestAdapter.length());

        
        AdapterRemoved = 1;}
    else if ((AdapterFound == false) && (AdapterFound2 == true)){
        
            PrintLongestArray.push_back(LongestAdapter2);
            PrintLongestArrayLength.push_back(LongestAdapter2.length());

            
        AdapterRemoved = 2;}
    else {
        AdapterRemoved = 0;}

    
    for (int i = 0; i < PrintLongestArray.size(); i++) {
        std::cout << "Longest Array " << PrintLongestArray[i] << " With Size " << PrintLongestArrayLength[i] << '\n';
